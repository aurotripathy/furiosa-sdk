{
  "title": "Artifact",
  "description": "Data including model binary, metadata, and configurations to run a single model.",
  "type": "object",
  "properties": {
    "name": {
      "title": "Name",
      "type": "string"
    },
    "family": {
      "title": "Family",
      "type": "string"
    },
    "location": {
      "title": "Location",
      "type": "string"
    },
    "format": {
      "$ref": "#/definitions/Format"
    },
    "description": {
      "title": "Description",
      "type": "string"
    },
    "config": {
      "$ref": "#/definitions/RuntimeConfig"
    },
    "metadata": {
      "$ref": "#/definitions/ModelMetadata"
    }
  },
  "required": [
    "name",
    "family",
    "location",
    "format"
  ],
  "definitions": {
    "Format": {
      "title": "Format",
      "description": "Model binary format to represent the binary specified.",
      "enum": [
        "code",
        "onnx",
        "tflite"
      ],
      "type": "string"
    },
    "RuntimeConfig": {
      "title": "RuntimeConfig",
      "description": "Runtime configurations for Furiosa Nux runtime.",
      "type": "object",
      "properties": {
        "npu_device": {
          "title": "Npu Device",
          "type": "string"
        },
        "compiler_config": {
          "title": "Compiler Config",
          "type": "object"
        }
      }
    },
    "ModelMetadata": {
      "title": "ModelMetadata",
      "description": "Model metadata to understand a model.",
      "type": "object",
      "properties": {
        "task": {
          "title": "Task",
          "type": "string"
        },
        "input_shapes": {
          "title": "Input Shapes",
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          }
        },
        "output_shapes": {
          "title": "Output Shapes",
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          }
        },
        "num_ops": {
          "title": "Num Ops",
          "type": "integer"
        },
        "num_op_types": {
          "title": "Num Op Types",
          "type": "integer"
        },
        "ops": {
          "title": "Ops",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "num_params": {
          "title": "Num Params",
          "type": "integer"
        },
        "num_macs": {
          "title": "Num Macs",
          "type": "integer"
        },
        "arxiv": {
          "title": "Arxiv",
          "type": "string"
        },
        "year": {
          "title": "Year",
          "type": "integer"
        },
        "month": {
          "title": "Month",
          "type": "integer"
        }
      }
    }
  }
}
